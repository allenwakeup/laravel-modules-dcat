{
    "name": "goodcatch/laravel-modules-dcat",
    "description": "Dcat Admin modules resources",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "allen",
            "email": "ali@goodcatch.cn",
            "homepage": "https://www.goodcatch.cn/laravel-modules-dcat",
            "role": "Developer"
        }
    ],

    "require": {
        "php": ">=7.3",
        "ext-json": "*",
        "goodcatch/laravel-modules": "dev-master"
    },
    "require-dev": {
        "elendev/nexus-composer-push": "^0.2.2",
        "illuminate/support": "^8.0",
        "laravel/framework": "^8.0",
        "laravel/helpers": "^1.1",
        "laravel/laravel": "^8.0",
        "dcat/laravel-admin": "2.*",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "Goodcatch\\Modules\\Dcat\\": "src"
        },
        "classmap": [
            "database/seeds"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "nexus-push": {
            "url": "https://repos.goodcatch.cn/repository/composer",
            "ignore-by-git-attributes": true,
            "ignore": [
                "test.php",
                "auth.json",
                "composer.lock",
                ".gitignore",
                ".git",
                ".DS_Store",
                ".idea/",
                "/vendor"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repos.goodcatch.cn/repository/goodcatch-composer-group/"
        },
        {
            "packagist.org": false
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
